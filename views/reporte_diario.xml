        <xpath expr="//table/t/tbody/t/tr/td[2]" position="replace">
            <td>
                <span t-esc="l['asiento_contable']"/>
            </td>
        </xpath>
        <xpath expr="//table/t/tbody/t/tr/td[3]" position="replace">
